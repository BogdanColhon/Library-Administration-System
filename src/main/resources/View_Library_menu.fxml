               <String fx:value="All" />
               <String fx:value="Adventure" />
               <String fx:value="Detective" />
               <String fx:value="War" />
            </FXCollections>
         </items>
      </ComboBox>
      <Label fx:id="Message" layoutX="167.0" layoutY="391.0" prefHeight="26.0" prefWidth="250.0" textFill="DARKSLATEGRAY">
